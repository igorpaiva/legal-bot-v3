{
  "name": "whatsapp-bot-admin",
  "version": "1.0.0",
  "description": "WhatsApp bot with admin panel and Groq LLM integration",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "clean:empty": "./remove-empty-files.sh",
    "clean:db": "./clean_system.sh"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^7.0.0-rc.3",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^159.0.0",
    "groq-sdk": "^0.5.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "marked": "^16.2.1",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "pdf2json": "^3.2.0",
    "pino": "^9.10.0",
    "playwright": "^1.55.0",
    "puppeteer": "^24.19.0",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^2.4.2",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "whatsapp-web.js": "^1.23.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
